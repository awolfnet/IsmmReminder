<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <!-- Reminder to acknowledge since the fault reported in minutes -->
    <add key="MinutesToAcknowledge" value="60"/>

    <!-- Reminder to complete since the fault reported in minutes -->
    <add key="MinutesToComplete" value="240"/>

    <!-- The past days' orders to fetch-->
    <add key="DaysToFetch" value="-14"/>
  </appSettings>
</configuration>